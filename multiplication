import java.util.Scanner;
 
public class Main {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.print("Enter Number: ");
		int c = in.nextInt();
        
        int product; 
		for(int i=0; i<=10 ; i++) {
		    product = c*i;
			System.out.println(c+"x"+i+"="+product);
			}
	}
}
